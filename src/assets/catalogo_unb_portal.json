[
	{
		"name": "/portal/assets/",
		"comment": "Bibliotecas node_modules do portal",
		"owner": "portal",
		"version": "1.0.0",
		"service" : "ems_static_file_service:execute",
		"url": "/portal/assets/",
		"use_re" : true,
		"path" : "{{ unb_portal }}",
		"type": "GET",
		"cache_control" : "max-age=86400, public",
		"expires_minute" : 1440,
		"lang" : "erlang",
		"authorization" : "public"
	},

	{
		"name": "/portal/images",
		"comment": "image static files of seguranca",
		"owner": "portal",
		"version": "1.0.0",
		"service" : "ems_static_file_service:execute",
		"url": "/portal/app/.*/img/",
		"use_re" : true,
		"path" : "{{ unb_portal }}",
		"type": "GET",
		"cache_control" : "max-age=3600, public",
		"expires_minute" : 60,
		"lang" : "erlang",
		"authorization" : "public"
	},


	{
		"name": "redirect_url_seguranca",
		"comment": "Redirect url /portal to /portal/index.html",
		"owner": "portal",
		"version": "1.0.0",
		"service" : "ems_redirect_url_service:execute",
		"url": "/portal",
		"redirect_url" : "/portal/index.html",
		"path" : "{{ unb_portal }}",
		"type": "GET",
		"cache_control" : "no-cache",
		"expires_minute" : 0,
		"lang" : "erlang",
		"authorization" : "public"
	},

	{
		"name": "/portal",
		"comment": "Sistema de Seguranca ErlangMS",
		"owner": "portal",
		"version": "1.0.0",
		"service" : "ems_static_file_service:execute",
		"url": "/portal/",
		"use_re" : true,
		"path" : "{{ unb_portal }}",
		"type": "GET",
		"cache_control" : "no-cache",
		"expires_minute" : 0,
		"lang" : "erlang",
		"authorization" : "public"
	}
]
