[
	{
		"name": "/dashboard/sobre",
		"comment": "Dashboard",
		"owner": "portal",
		"version": "1.0.0",
		"service" : "ems_portal_dashboard:sobre",
		"url": "/dashboard/sobre",
		"path" : "{{ portal_api_management }}",
		"type": "GET",
		"lang" : "erlang"
	},

	{
		"name": "/portal/node_modules",
		"comment": "Bibliotecas node_modules do portal",
		"owner": "portal",
		"version": "1.0.0",
		"service" : "ems_static_file_service:execute",
		"url": "/portal/node_modules/",
		"use_re" : true,
		"path" : "{{ portal_api_management }}",
		"type": "GET",
		"cache_control" : "max-age=86400, public",
		"expires_minute" : 1440,
		"authorization" : "public",
		"lang" : "erlang"
	},

	{
		"name": "/portal/images",
		"comment": "image static files of portal",
		"owner": "portal",
		"version": "1.0.0",
		"service" : "ems_static_file_service:execute",
		"url": "/portal/app/.*/img/",
		"use_re" : true,
		"path" : "{{ portal_api_management }}",
		"type": "GET",
		"cache_control" : "max-age=3600, public",
		"expires_minute" : 60,
		"authorization" : "public",
		"lang" : "erlang"
	},

	{
		"name": "redirect_url_portal",
		"comment": "Redirect url /portal to /portal/index.html",
		"owner": "portal",
		"version": "1.0.0",
		"service" : "ems_redirect_url_service:execute",
		"url": "/portal",
		"redirect_url" : "/portal/index.html",
		"path" : "{{ portal_api_management }}",
		"type": "GET",
		"cache_control" : "no-cache",
		"expires_minute" : 0,
		"authorization" : "public",
		"lang" : "erlang"
	},

	{
		"name": "/portal",
		"comment": "Portal ErlangMS",
		"owner": "portal",
		"version": "1.0.0",
		"service" : "ems_django_service:execute",
		"url": "/portal/",
		"use_re" : true,
		"path" : "{{ portal_api_management }}",
		"type": "GET",
		"cache_control" : "no-cache",
		"expires_minute" : 0,
		"authorization" : "public",
		"lang" : "erlang"
	}
]
